  <description/>
  <content:encoded><![CDATA[<!-- wp:cover {"url":"https://soloreadermycanva.files.wordpress.com/2024/05/pexels-photo-2395249.jpeg?w=1024","dimRatio":40,"isDark":false,"align":"center","style":{"color":{"duotone":"unset"},"spacing":{"margin":{"top":"0","bottom":"0"},"blockGap":"var:preset|spacing|10"},"border":{"width":"0px","style":"none","radius":"42px"}},"layout":{"type":"default"},"fontSize":"medium"} -->
